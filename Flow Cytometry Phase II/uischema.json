{
    "type": "VerticalLayout",
    "elements": [
      {
        "type": "Label",
        "text": "Flow Cytometry Gated Data Upload"
      },
      {
        "type": "HorizontalLayout",
        "elements": [
          {
            "type": "Control",
            "scope": "#/properties/myLabDataTaskId",
            "label": "MyLabData TaskId",
            "options": {
              "renderer": "custom-string"
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/experimentNumber",
            "label": "Experimant Number (ELN)",
            "options": {
              "renderer": "custom-string"
            }
          }
        ]
      },
      {
        "type": "HorizontalLayout",
        "elements": [
          {
            "type": "Control",
            "scope": "#/properties/instrumentSapId",
            "label": "Instrument SAP ID",
            "options": {
              "renderer": "custom-string"
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/requesterMUDID",
            "label": "Requester MUDID",
            "options": {
              "renderer": "custom-string"
            }
          }
        ]
      },
      {
        "type": "HorizontalLayout",
        "elements": [
          {
            "type": "Control",
            "scope": "#/properties/projectName",
            "label": "Project Name",
            "options": {
              "renderer": "custom-string"
            }
          },
          {
            "type": "Control",
            "scope": "#/properties/resultRecipient",
            "label": "Result Recipient (type first and last name)",
            "options": {
              "renderer": "custom-email-list"
            }
          }
        ]
      },
      {
        "type": "HorizontalLayout",
        "elements": [
          {
            "type": "Control",
            "scope": "#/properties/studyNumber",
            "label": "Study Number",
            "options": {
              "renderer": "custom-string"
            }
          }
        ]
      },
      {
        "type": "HorizontalLayout",
        "elements": [
          {
            "type": "VerticalLayout",
            "elements": [
              {
                "type": "Control",
                "scope": "#/properties/typeflocyto",
                "label": "",
                "options": {
                  "renderer": "custom-radio-vertical"
                }
              }
            ]
          },
          {
            "type": "VerticalLayout",
            "elements": [
              {
                "type": "Control",
                "label": "FlowJo .wsp file in MyLabData",
                "scope": "#/properties/flowjofile",
                "options": {
                  "renderer": "custom-string"
                },
                "rule": {
                  "effect": "ENABLE",
                  "condition": {
                    "scope": "#/properties/typeflocyto",
                    "schema": {
                      "const": "FlowJo"
                    }
                  }
                }
              },
              {
                "type": "Control",
                "label": "Cytobank Experiment ID",
                "scope": "#/properties/cytobankexptid",
                "options": {
                  "renderer": "custom-string"
                },
                "rule": {
                  "effect": "ENABLE",
                  "condition": {
                    "scope": "#/properties/typeflocyto",
                    "schema": {
                      "const": "Cytobank"
                    }
                  }
                }
              }
            ]
          }
        ]
      },
      {
        "type": "HorizontalLayout",
        "elements": [
          {
            "type": "Control",
            "label": "FloJo or Cytobank",
            "scope": "#/properties/floworcyto",
            "options": {
              "renderer": "custom-anyof-string"
            }
          }
        ]
      }
    ]
  }
  