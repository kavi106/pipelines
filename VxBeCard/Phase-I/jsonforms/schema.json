{
  "type": "object",
  "properties": {
    "myLabDataTaskId": {
      "type": "string",
      "name": "MyLabData TaskId",
      "pattern": "^[A-Za-z0-9_-]{0,30}$",
      "maxLength": 30
    },
    "requesterMUDID": {
      "type": "string",
      "name": "Requester MUDID",
      "pattern": "^[A-Za-z0-9]{0,15}$",
      "maxLength": 15
    },
    "instrumentSapId": {
      "type": "number",
      "name": "Instrument SAP ID",
      "pattern": "^[0-9]{0,20}$",
      "maxLength": 20
    },
    "dataCleanAlgorithm": {
      "type": "string",
      "name": "Data Clean Algorithm",
      "pattern": "^.{0,20}$",
      "enum": ["FlowAI (1.30.0)","FlowCut (1.10.0)","PeacoQC (1.10.0)"]
    },
    "projectName": {
      "type": "string",
      "name": "Project Name",
      "pattern": "^.{0,50}$",
      "maxLength": 50
    },
    "experimentNumber": {
      "type": "string",
      "name": "Experiment Number",
      "pattern": "^ELN[0-9a-zA-Z-_]{0,20}$",
      "maxLength": 23
    },
    "studyNumber": {
      "type": "string",
      "name": "Study Number",
      "pattern": "^[0-9a-zA-Z_-]{0,30}$",
      "maxLength": 30
    },
    "resultRecipient": {
      "type": "array",
      "name": "Result Recipient",
      "pattern": "^[a-zA-Z0-9_.]+@[a-zA-Z_]+?\\.[a-zA-Z]{2,3}$"
    }
  },
  "required": [
    "myLabDataTaskId",
    "requesterMUDID",
    "instrumentSapId",
    "dataCleanAlgorithm",
    "projectName",
    "experimentNumber",
    "studyNumber"
  ]
}