{
  "type": "object",
  "properties": {
    "myLabDataTaskId": {
      "type": "string",
      "maxLength": 200
    },
    "requesterMUDID": {
      "type": "string",
      "maxLength": 200
    },
    "instrumentSapId": {
      "type": "number",
      "maxLength": 200
    },
    "dataCleanAlgorithm": {
      "type": "string",
      "enum": ["FlowAI (1.30.0)","FlowCut (1.10.0)","PeacoQC (1.10.0)"]
    },
    "projectId": {
      "type": "string",
      "maxLength": 200
    },
    "experimentNumber": {
      "type": "string",
      "maxLength": 200,
      "pattern": "^0(9|4)\\d{8}$"
    },
    "studyNumber": {
      "type": "string",
      "maxLength": 200
    },
    "resultRecipient": {
      "type": "array"
    }
  },
  "required": [
    "myLabDataTaskId",
    "requesterMUDID",
    "instrumentSapId",
    "dataCleanAlgorithm",
    "projectId",
    "experimentNumber",
    "studyNumber"
  ]
